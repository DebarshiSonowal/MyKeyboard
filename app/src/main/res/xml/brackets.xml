    <Row>
        <Key android:codes="95" android:keyIcon="@drawable/underscore" android:keyEdgeFlags="left"/>
        <Key android:codes="94" android:keyLabel="^"/>
        <Key android:codes="60" android:keyIcon="@drawable/lessthan"/>
        <Key android:codes="62" android:keyIcon="@drawable/greaterthan" android:keyEdgeFlags="right"/>
