<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="20%p"
    android:horizontalGap="5dp"
    android:verticalGap="5dp"
    android:keyHeight="50dp"
    android:fontFamily="@font/font1"
    android:theme="@style/AppTheme">
    <Row>
        <Key android:codes="123" android:keyIcon="@drawable/curvebracket1" android:keyEdgeFlags="left" />
        <Key android:codes="125" android:keyIcon="@drawable/curvebracket2"/>
        <Key android:codes="91" android:keyIcon="@drawable/squarebracket1"/>
        <Key android:codes="93" android:keyIcon="@drawable/squarebracket2"  android:keyEdgeFlags="right"/>
