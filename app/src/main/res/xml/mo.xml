        <Key android:codes="0xffc06" android:keyIcon="@drawable/ma6"  android:keyWidth="30%p" android:keyEdgeFlags="left"/>
        <Key android:codes="0xffc07" android:keyIcon="@drawable/ma7"  android:keyWidth="30%p"/>
        <Key android:codes="0xffc08" android:keyIcon="@drawable/ma8"  android:keyWidth="30%p"/>
        <Key android:codes="0xffc09" android:keyIcon="@drawable/ma9"  android:keyWidth="30%p" android:keyEdgeFlags="right"/>
