<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="5dp"
    android:verticalGap="5dp"
    android:keyHeight="50dp"
    android:keyWidth="16%p"
    android:fontFamily="@font/font1"
    android:theme="@style/AppTheme">
    <Row android:rowEdgeFlags="top">
        <Key android:codes="38" android:keyLabel="&amp;" android:keyEdgeFlags="left"/>
        <Key android:codes="34" android:keyIcon="@drawable/ic_dari_group_quotedouble_u_0022"/>
        <Key android:codes="58" android:keyIcon="@drawable/colon"/>
        <Key android:codes="64" android:keyLabel="\@"/>
        <Key android:codes="92" android:keyIcon="@drawable/backslash" android:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key android:codes="39" android:keyIcon="@drawable/quotesingle" android:keyEdgeFlags="left"/>
        <Key android:codes="95" android:keyIcon="@drawable/underscore" />
        <Key android:codes="37" android:keyIcon="@drawable/percent" />
        <Key android:codes="47"  android:keyIcon="@drawable/slash"  />
        <Key android:codes="42" android:keyIcon="@drawable/asterisk" android:keyEdgeFlags="right"/>
    </Row>
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="59" android:keyIcon="@drawable/semicolon" android:keyEdgeFlags="left"/>
        <Key android:codes="46" android:keyIcon="@drawable/period" />
        <Key android:codes="35" android:keyIcon="@drawable/hashtag" />
        <Key android:codes="33" android:keyIcon="@drawable/exclam"/>
        <Key android:codes="63" android:keyIcon="@drawable/questionmark" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>